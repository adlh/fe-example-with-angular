{
    "rulesDirectory": [
        "node_modules/codelyzer",
        "node_modules/tslint-clean-code/dist/build"
    ],
    "rules": {
        "callable-types": true,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "curly": true,
        "eofline": true,
        "forin": true,
        "import-blacklist": [
            true,
            "rxjs"
        ],
        "import-spacing": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-over-type-literal": false,
        "label-position": true,
        "max-line-length": [
            true,
            140
        ],
        "member-access": false,
        "member-ordering": [
            true,
            {
                "order": "fields-first"
            }
        ],
        "no-arg": true,
        "no-bitwise": true,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-empty-interface": true,
        "no-eval": true,
        "no-inferrable-types": [
            true,
            "ignore-params"
        ],
        "no-shadowed-variable": true,
        "no-string-literal": false,
        "no-string-throw": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "object-literal-sort-keys": false,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "prefer-const": true,
        "quotemark": [
            true,
            "single"
        ],
        "radix": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef-whitespace": false,
        "typeof-compare": true,
        "unified-signatures": true,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "directive-selector": [
            false,
            "attribute",
            "app",
            "camelCase"
        ],
        "use-input-property-decorator": true,
        "use-output-property-decorator": true,
        "use-host-property-decorator": true,
        "no-input-rename": true,
        "no-output-rename": true,
        "use-life-cycle-interface": true,
        "use-pipe-transform-interface": true,
        "component-class-suffix": false,
        "directive-class-suffix": false,
        "no-access-missing-member": true,
        "templates-use-public": true,
        "invoke-injectable": true,
        "await-promise": true,
        "id-length": false,
        "match-default-export-name": true,
        "max-func-args": [
            true,
            3
        ],
        "min-class-cohesion": [
            true,
            0.5
        ],
        "newspaper-order": true,
        "no-unsafe-any": false,
        "no-conditional-assignment": true,
        "no-duplicate-super": true,
        "no-flag-args": true,
        "no-floating-promises": false,
        "no-for-each-push": true,
        "no-for-in-array": true,
        "no-import-side-effect": false,
        "no-invalid-template-strings": true,
        "no-invalid-this": true,
        "no-map-without-usage": true,
        "no-misused-new": true,
        "no-non-null-assertion": true,
        "no-reference-import": true,
        "no-sparse-arrays": true,
        "no-unnecessary-callback-wrapper": true,
        "no-unnecessary-initializer": true,
        "no-unsafe-finally": true,
        "promise-function-async": false,
        "restrict-plus-operands": true,
        "strict-boolean-expressions": [
            true,
            "allow-undefined-union",
            "allow-number"
        ],
        "switch-default": true,
        "try-catch-first": true,
        "use-isnan": true,
        "adjacent-overload-signatures": true,
        "array-type": [
            true,
            "array"
        ],
        "arrow-parens": false,
        "interface-name": false,
        "jsdoc-format": true,
        "max-classes-per-file": [
            true,
            2
        ],
        "max-file-line-count": [
            true,
            300
        ],
        "new-parens": true,
        "no-complex-conditionals": true,
        "no-default-export": true,
        "no-feature-envy": [
            true,
            2,
            [
                "_"
            ]
        ],
        "no-null-keyword": false,
        "no-parameter-properties": false,
        "no-require-imports": true,
        "no-unnecessary-qualifier": true,
        "no-var-requires": true,
        "no-void-expression": true,
        "one-variable-per-declaration": true,
        "only-arrow-functions": false,
        "ordered-imports": false,
        "prefer-dry-conditionals": true,
        "prefer-for-of": true,
        "prefer-method-signature": true,
        "prefer-template": true,
        "return-undefined": false,
        "align": [
            true,
            "parameters",
            "arguments",
            "statements"
        ],
        "linebreak-style": [
            true,
            "LF"
        ],
        "newline-before-return": true,
        "no-consecutive-blank-lines": [
            true,
            1
        ],
        "object-literal-key-quotes": false,
        "ban": false,
        "ban-types": true,
        "cyclomatic-complexity": true,
        "file-header": false,
        "no-angle-bracket-type-assertion": false,
        "no-inferred-empty-object-type": false,
        "no-internal-module": true,
        "no-magic-numbers": true,
        "no-mergeable-namespace": false,
        "no-namespace": false,
        "no-reference": true,
        "object-literal-shorthand": false,
        "space-before-function-paren": false
    }
}
